	keywords = {Transcription, Genetic, Transcription Factors, Biosensing Techniques, Gene Expression Regulation, Luminescent Proteins, Metabolic Engineering, Metabolic Flux Analysis, Models, Genetic},
	pages = {7648--7660},
	file = {Full Text:/Users/jonahschwartz/Zotero/storage/EBVQGVXJ/Rogers et al. - 2015 - Synthetic biosensors for precise gene control and .pdf:application/pdf},
