	editor = {Harper, David R. and Abedon, Stephen T. and Burrowes, Benjamin H. and McConville, Malcolm L.},
	year = {2021},
	doi = {10.1007/978-3-319-41986-2_1},
	pages = {19--91},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/DF2IACKX/Sanz-Gaitero et al. - 2021 - Structure and Function of Bacteriophages.pdf:application/pdf},
}

