	pages = {583--589},
	file = {Jumper et al. - 2021 - Highly accurate protein structure prediction with AlphaFold  Nature.pdf:/Users/jonahschwartz/Zotero/storage/EHMK2ZSS/Jumper et al. - 2021 - Highly accurate protein structure prediction with AlphaFold  Nature.pdf:application/pdf},
}

@article{geri_microenvironment_2020,
