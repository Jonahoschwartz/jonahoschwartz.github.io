	keywords = {Humans, Binding Sites, Receptors, Glucocorticoid, Transcriptional Activation, Transcription Factors, A549 Cells, Chromatin Immunoprecipitation, Dexamethasone, Genes, Reporter, Genome, Human, Glucocorticoids, Protein Binding, Response Elements},
	pages = {1269--1281.e19},
