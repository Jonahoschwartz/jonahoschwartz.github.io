	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/cbic.201800668},
	keywords = {genetic code, origin of proteins, primordial peptides, protein libraries},
	pages = {846--856},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/ZGCDYAPK/Newton et al. - 2019 - Genetic Code Evolution Investigated through the Sy.pdf:application/pdf;Snapshot:/Users/jonahschwartz/Zotero/storage/JXB57FLA/cbic.html:text/html},
