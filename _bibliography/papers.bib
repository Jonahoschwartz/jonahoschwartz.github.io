	journal = {Frontiers in Bioengineering and Biotechnology},
	author = {Liang, Guo-Teng and Lai, Cuixin and Yue, Zejun and Zhang, Hanbin and Li, Danyang and Chen, Zhong and Lu, Xingyu and Tao, Liang and Subach, Fedor V. and Piatkevich, Kiryl D.},
