	journal = {Biochimica et Biophysica Acta (BBA) - Proteins and Proteomics},
	author = {Palm, Gottfried Julius and Buchholz, Ina and Werten, Sebastiaan and Girbardt, Britta and Berndt, Leona and Delcea, Mihaela and Hinrichs, Winfried},
	month = jun,
