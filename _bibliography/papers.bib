	pmid = {25297546},
	pmcid = {PMC4434928},
	pages = {122},
	file = {PubMed Central Full Text PDF:/Users/jonahschwartz/Zotero/storage/7F74GN45/Szwarc et al. - 2014 - The p160Steroid Receptor Coactivator Family Pote.pdf:application/pdf},
}

