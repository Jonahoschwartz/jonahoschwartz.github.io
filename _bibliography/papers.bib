	author = {Makarov, Mikhail and Sanchez Rocha, Alma C. and Krystufek, Robin and Cherepashuk, Ivan and Dzmitruk, Volha and Charnavets, Tatsiana and Faustino, Anneliese M. and Lebl, Michal and Fujishima, Kosuke and Fried, Stephen D. and Hlouchova, Klara},
	month = mar,
	year = {2023},
	note = {Publisher: American Chemical Society},
	pages = {5320--5329},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/8ZQ3STP5/Makarov et al. - 2023 - Early Selection of the Amino Acid Alphabet Was Ada.pdf:application/pdf},
}

