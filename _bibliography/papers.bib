	urldate = {2023-11-28},
	journal = {Journal of the American Chemical Society},
	author = {Xia, Yan and DiPrimio, Nina and Keppel, Theodore R. and Vo, Binh and Fraser, Keith and Battaile, Kevin P. and Egan, Chet and Bystroff, Christopher and Lovell, Scott and Weis, David D. and Anderson, J. Christopher and Karanicolas, John},
