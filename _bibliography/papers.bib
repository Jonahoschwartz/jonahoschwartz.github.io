	month = mar,
	year = {2016},
	note = {Publisher: Proceedings of the National Academy of Sciences},
	pages = {2544--2549},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/QPEJ7LRF/Wong et al. - 2016 - Multiplexed barcoded CRISPR-Cas9 screening enabled.pdf:application/pdf},
}

