	keywords = {Drug development, Biotechnology, Business strategy in drug development, Clinical trials, Drug therapy},
	pages = {19--34},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/KJBHUHVQ/Santos et al. - 2017 - A comprehensive map of molecular drug targets.pdf:application/pdf},
}

