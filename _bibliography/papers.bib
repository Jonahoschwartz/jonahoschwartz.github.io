	note = {Publisher: American Chemical Society},
	pages = {81--92},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/V2KQDMIT/Agostini et al. - 2021 - Multiomics Analysis Provides Insight into the Labo.pdf:application/pdf},
