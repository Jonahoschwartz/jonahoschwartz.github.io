	author = {Robinson, Serina L. and Piel, Jörn and Sunagawa, Shinichi},
	pmid = {34821235},
	pmcid = {PMC8597712},
	pages = {1994--2023},
	file = {Full Text:/Users/jonahschwartz/Zotero/storage/UWIDKEP2/Robinson et al. - A roadmap for metagenomic enzyme discovery.pdf:application/pdf},
}

