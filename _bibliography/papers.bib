	journal = {ACS Synthetic Biology},
	author = {Kastberg, Louise La Barbera and Petrov, Mykhaylo S. and Strucko, Tomas and Jensen, Michael K. and Workman, Christopher T.},
	month = feb,
	year = {2024},
}

