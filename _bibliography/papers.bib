	keywords = {hypoxia, Copper, flavin, Fluorescent protein biosensor, live cell imaging microscopy},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/C25WGVUK/Liang et al. - 2022 - Enhanced small green fluorescent proteins as a mul.pdf:application/pdf},
