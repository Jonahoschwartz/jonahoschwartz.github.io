	title = {A parametrized two-domain thermodynamic model explains diverse mutational effects on protein allostery},
	copyright = {© 2023, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NoDerivs 4.0 International), CC BY-ND 4.0, as described at http://creativecommons.org/licenses/by-nd/4.0/},
	url = {https://www.biorxiv.org/content/10.1101/2023.08.06.552196v1},
	doi = {10.1101/2023.08.06.552196},
	abstract = {Our understanding of protein allostery continues to be challenged by new experimental observations. Specifically, recent deep mutational scanning study showed that allosteric hotspots in the tetracycline repressor (TetR) and its homologous transcriptional factors (TFs) are broadly distributed rather than spanning well-defined structural pathways as often assumed. Moreover, the loss of allostery due to mutations of the hotspots was rescued by distributed additional mutations in a degenerate fashion. To gain a physical understanding of these intriguing observations, we develop a two-domain thermo-dynamic model for TetR, in which the protein is generically divided into ligand- and DNA-binding domains. The model incorporates three biophysical parameters to capture the intra- and inter-domain properties of TetR, and readily rationalizes the distributed network of allosterically coupled residues. Furthermore, we establish a quantitative connection between the model and the in vivo expression level of the TetR-regulated gene, which allows the extraction of the biophysical parameters in diverse mutants based on experimental induction curves using statistical inference. The quantitative mapping of the mutants to the model parameter space reveals the intrinsic connection between intra- and inter-domain allosteric parameters and also enables the evaluation of epistasis between the characterized mutations. The insights gained from this study into the nature of two-domain allostery are expected to have broader implications for other multidomain allosteric proteins.},
	language = {en},
	urldate = {2023-08-22},
	publisher = {bioRxiv},
	author = {Liu, Zhuang and Gillis, Thomas and Raman, Srivatsan and Cui, Qiang},
	month = aug,
	year = {2023},
	note = {Pages: 2023.08.06.552196
Section: New Results},
	keywords = {Biorixv, allostery, Model, TetR},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/EYH49KNS/Liu et al. - 2023 - A parametrized two-domain thermodynamic model expl.pdf:application/pdf},
}

@article{heery_signature_1997,
