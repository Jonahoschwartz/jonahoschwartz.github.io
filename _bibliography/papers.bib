	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/DYJ8TSDL/Qing et al. - 2022 - Protein Design From the Aspect of Water Solubilit.pdf:application/pdf},
}

@misc{sumida_improving_2023,
