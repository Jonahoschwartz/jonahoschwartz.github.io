	year = {2021},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Biological techniques, Systems biology, Computational biology and bioinformatics},
	pages = {1175},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/NB2Z7S48/Srivastava et al. - 2021 - Transcriptome-wide high-throughput mapping of prot.pdf:application/pdf},
}

