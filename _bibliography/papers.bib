	note = {Publisher: Public Library of Science},
	keywords = {Sequence motif analysis, Gene regulation, Transcriptional control, Cell binding, Binding analysis, Deoxyribonucleases, Mammalian genomics, Steroids},
	pages = {e94},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/F6QMPFV6/So et al. - 2007 - Determinants of Cell- and Gene-Specific Transcript.pdf:application/pdf},
}

