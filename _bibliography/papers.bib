	keywords = {Transcription factors, Antibacterial drug resistance, Bacterial genetics},
	pages = {1444},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/CJFBTBWH/Sharma et al. - 2017 - The multiple antibiotic resistance operon of enter.pdf:application/pdf},
}

