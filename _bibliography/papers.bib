@article{yin_genome-wide_2012,
	title = {Genome-{Wide} {Progesterone} {Receptor} {Binding}: {Cell} {Type}-{Specific} and {Shared} {Mechanisms} in {T47D} {Breast} {Cancer} {Cells} and {Primary} {Leiomyoma} {Cells}},
