	keywords = {Animals, Humans, Models, Molecular, Protein Structure, Tertiary, Transcriptional Activation, Cell Nucleus, DNA-Binding Proteins, Nuclear Proteins, Protein Interaction Mapping, Receptors, Cytoplasmic and Nuclear, Recombinant Proteins, Structure-Activity Relationship, Transcription Factors},
	pages = {1--10},
	file = {Full Text:/Users/jonahschwartz/Zotero/storage/3P7Y7S5S/Kumar and Thompson - 2003 - Transactivation functions of the N-terminal domain.pdf:application/pdf},
