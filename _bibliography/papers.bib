	note = {Publisher: Public Library of Science},
	keywords = {Genomics, DNA-binding proteins, Sequence motif analysis, Transcription factors, Binding analysis, Amino acid analysis, Nucleotides, Operons},
	pages = {e0132618},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/DXUIFJ7T/Suvorova et al. - 2015 - GntR Family of Bacterial Transcription Factors and.pdf:application/pdf},
}

@article{madan_babu_evolution_2003,
