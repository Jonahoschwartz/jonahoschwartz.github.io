	month = sep,
	year = {2021},
	note = {Publisher: Nature Publishing Group},
	keywords = {Synthetic biology, Protein design},
	pages = {5592},
	file = {Full Text PDF:/Users/jonahschwartz/Zotero/storage/92NU2C53/Jiang et al. - 2021 - Coevolutionary methods enable robust design of mod.pdf:application/pdf},
}

